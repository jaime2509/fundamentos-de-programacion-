using namespace std;

void main()
{

   float a,area;
   // Pedir el radio 
   cout<<"Ingrese el radio: ";
   cin>>a;
   
   
   area =(a*a)*3.1416;//Proceso 
   // Salida
   cout<<"El area del circulo es: "<<area;
   getch(); 

}
